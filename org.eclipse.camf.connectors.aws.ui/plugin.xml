<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.camf.ui.newCloudProviderWizards">
      <wizard
            class="org.eclipse.camf.connectors.aws.ui.wizards.AWSCloudProviderWizard"
            id="org.eclipse.camf.connectors.aws.ui.wizards.awsCloudProviderWizard"
            name="AWS Cloud Provider">
      </wizard>
   </extension>
<extension
         point="org.eclipse.camf.ui.authTokenUI">
      <wizard
            class="org.eclipse.camf.connectors.aws.ui.wizards.AWSAuthTokenWizard"
            icon="icons/obj16/proxy.gif"
            id="org.eclipse.camf.connectors.aws.ui.wizards.awsAuthTokenWizard"
            name="AWS Auth Token">
      </wizard>
      <factory
            class="org.eclipse.camf.connectors.aws.ui.dialogs.AWSAuthTokenUIFactory"
            id="org.eclipse.camf.connectors.aws.ui.awsAuthTokenUIFactory"
            name="AWS Auth Token UI Factory">
      </factory>
   </extension>
<extension
      point="org.eclipse.camf.ui.cloudProviderIcon">
   <icon
         location="icons/obj16/cp.gif"
         type="org.eclipse.camf.connectors.aws">
   </icon>
</extension>

</plugin>
